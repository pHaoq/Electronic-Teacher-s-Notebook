<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="springbootapp.gui.StudentGradesController"
      spacing="10" alignment="CENTER">

    <!-- Back Button -->
    <Button fx:id="backButton" text="Back" onAction="#handleBackButton" />

    <!-- Tabelle fÃ¼r Studentengrades -->
    <TableView fx:id="studentsTable" prefHeight="400" prefWidth="600">
        <columns>
            <TableColumn fx:id="studentIdColumn" text="Student ID" prefWidth="100"/>
            <TableColumn fx:id="fullNameColumn" text="Full Name" prefWidth="300"/>
            <TableColumn fx:id="gradesColumn" text="Grades" prefWidth="300"/>
        </columns>
    </TableView>

</VBox>
