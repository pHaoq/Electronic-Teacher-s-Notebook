<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="springbootapp.gui.CoursesController" spacing="10">
    <TableView fx:id="coursesTable" prefHeight="400" prefWidth="600">
        <columns>
            <TableColumn fx:id="courseIdColumn" text="Course ID"/>
            <TableColumn fx:id="courseNameColumn" text="Name"/>
        </columns>
    </TableView>
    <Button text="Logout" onAction="#handleLogoutButton"/> <!-- Logout-Button -->
</VBox>
